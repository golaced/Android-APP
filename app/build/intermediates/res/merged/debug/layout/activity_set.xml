<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    >


    <Button
        android:id="@+id/b_back_gps"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="返回"
        android:textColor="@color/white"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="加速度"
        android:id="@+id/checkBox_ax"
        android:checked="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/checkBox_ypr"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="读取"
        android:id="@+id/ib_read"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/ib_write"
        android:layout_toStartOf="@+id/ib_write" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="陀螺仪"
        android:id="@+id/checkBox_gx"
        android:checked="true"
        android:layout_below="@+id/b_back_gps"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="磁力计"
        android:id="@+id/checkBox_hm"
        android:checked="true"
        android:layout_below="@+id/checkBox_gx"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="写入"
        android:id="@+id/ib_write"
        android:layout_alignTop="@+id/ib_read"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="姿态角"
        android:id="@+id/checkBox_ypr"
        android:checked="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/checkBox_hm"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="飞控发送数据"
        android:id="@+id/textView29"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GPS数据"
        android:id="@+id/checkBox_gps"
        android:checked="true"
        android:layout_alignTop="@+id/checkBox_gx"
        android:layout_toRightOf="@+id/checkBox_gx"
        android:layout_toEndOf="@+id/checkBox_gx" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="高度"
        android:id="@+id/checkBox_high"
        android:checked="true"
        android:layout_below="@+id/checkBox_gx"
        android:layout_toRightOf="@+id/checkBox_gx"
        android:layout_toEndOf="@+id/checkBox_gx" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="飞控模式"
        android:id="@+id/textView28"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GPS定位"
        android:id="@+id/Switch_gps"
        android:checked="false"
        android:layout_below="@+id/b_back_gps"
        android:layout_alignLeft="@+id/textView28"
        android:layout_alignStart="@+id/textView28" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="定高"
        android:id="@+id/Switch_high"
        android:checked="false"
        android:layout_below="@+id/Switch_gps"
        android:layout_alignRight="@+id/Switch_gps"
        android:layout_alignEnd="@+id/Switch_gps" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="惯性导航"
        android:id="@+id/Switch_ins"
        android:checked="false"
        android:layout_below="@+id/Switch_high"
        android:layout_alignRight="@+id/Switch_high"
        android:layout_alignEnd="@+id/Switch_high" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="锁尾"
        android:id="@+id/Switch_lockw"
        android:checked="false"
        android:layout_below="@+id/Switch_ins"
        android:layout_alignRight="@+id/Switch_ins"
        android:layout_alignEnd="@+id/Switch_ins" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="体感控制"
        android:id="@+id/Switch_imuc"
        android:checked="false"
        android:layout_below="@+id/Switch_lockw"
        android:layout_alignRight="@+id/Switch_lockw"
        android:layout_alignEnd="@+id/Switch_lockw" />

</RelativeLayout><!-- From: file:/F:/Quarotor/Source/UP_con/ANO_RC_v11/app/src/main/res/layout/activity_set.xml -->